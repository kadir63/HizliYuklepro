
<div class="container">
    <form class="form-horizontal" role="form" action="/Arayan/Kaydet" method="post">
        <h2>Nakliyeci Kayıt Formu</h2>
        <div class="form-group">
            <label for="firstName" class="col-sm-3 control-label">Ad</label>
            <div class="col-sm-9">
                <input type="text" id="ad" placeholder="ad" class="form-control" Ad value="Ad" autofocus name="ad">
            </div>
        </div>
        <div class="form-group">
            <label for="Soyad" class="col-sm-3 control-label">Soyad</label>
            <div class="col-sm-9">
                <input type="text" id="Soyad" placeholder="Soyad" class="form-control" name="Soyad" autofocus>
            </div>
        </div>
        <div class="form-group">
            <label for="Vergi Dairesi" class="col-sm-3 control-label">Vergi Dairesi* </label>
            <div class="col-sm-9">
                <input type="email" id="email" placeholder="Email" class="form-control" name="vergidairesi">
            </div>
        </div>
        <div class="form-group">
            <label for="Vergi No" class="col-sm-3 control-label">Vergi No*</label>
            <div class="col-sm-9">
                <input type="text" id="password" placeholder="Vergi No" class="form-control" name="vergino">
            </div>
        </div>
        <div class="form-group">
            <label for="Sifre" class="col-sm-3 control-label">şifre*</label>
            <div class="col-sm-9">
                <input type="password" id="password" placeholder="Password" class="form-control" name="sifre">
            </div>
        </div>
        <div class="form-group">
            <label for="Sifre" class="col-sm-3 control-label">şifre Tekrar*</label>
            <div class="col-sm-9">
                <input type="password" id="password" placeholder="Password" class="form-control" name="sifre2">
            </div>
        </div>
        <div class="form-group">
            <label for="birthDate" class="col-sm-3 control-label">Telefon*</label>
            <div class="col-sm-9">
                <input type="tel" id="telefon" class="form-control" name="telefon">
            </div>
        </div>

        <button type="submit" class="btn btn-primary btn-block">Kaydol</button>
        @Html.AntiForgeryToken()
    </form> <!-- /form -->
</div>
